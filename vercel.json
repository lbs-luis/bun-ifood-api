{
  "version": 2,
  "buildCommand": "bun run build:vercel",
  "rewrites": [
    {
      "src": "/public/:match*",
      "destination": "/:match*"
    },
    {
      "src": "/(.*)",
      "destination": "/api"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/http/server.ts"
    }
  ]
}